var num = ['Kelly','Agata','Julia','Lucivaldo','Marieta','Lucia','Lucimar','Luciano','Desirre','Sueli']
num.sort()

// a função indexOf() procura um item dentro da Array
console.log(`O nome Kelly está em qual posição? Está na posição ${num.indexOf('Kelly')}`)

// variavel.LENGTH diz o número de elementos da array
for(var pos = 0;pos<num.length;pos++) {
    console.log(`O item ${pos} é ${num[pos]}`)
}

// Simplificando looping da array
for (var pos = 0 in num){
    console.log(`O item ${pos} é ${num[pos]} - Simplificado!`)
}